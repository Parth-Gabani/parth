    <script>


        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
    <script>

        function delete_subattribute() {
            var delete_id = [];
            $.each($("input[name='check']:checked"), function () {
                delete_id.push($(this).val());
            });
            //alert("Id to Delete are: " + delete_id.join(", "));
            if (delete_id == "") {
                alert("Please Select a Subattribute to Delete");
            } else {

                if (confirm('Are You Sure You Want To Delete?')) {
                    $.ajax({
                        type: "POST",
                        url: "subattribute_delete?id=" + delete_id,
                        success: function (response) {
                            if (response == "error") {
                                alert("You cannot delete Subattribute as it is linked with catalog!");
                            } else if (response == "success") {
                                location.reload();
                            }
                        }
                    });
                }

            }
        }
    </script>

