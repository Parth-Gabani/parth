    <div class="page-loader">
        <div class="bg-primary"></div>
    </div>
    <!-- [ Preloader ] End -->

    <!-- [ Layout wrapper ] Start -->
    <div class="layout-wrapper layout-2">
        <div class="layout-inner">
            <!-- [ Layout sidenav ] Start -->
            <%- include('sidenav'); %>
                <!-- [ Layout sidenav ] End -->
                <!-- [ Layout container ] Start -->
                <div class="layout-container">
                    <!-- [ Layout navbar ( Header ) ] Start -->
                    <%- include('nav'); %>
                        <!-- [ Layout navbar ( Header ) ] End -->

                        <!-- [ Layout content ] Start -->
                        <div class="layout-content">

                            <!-- [ content ] Start -->
                            <div class="container-fluid flex-grow-1 container-p-y">
                                <h4 class="font-weight-bold py-3 mb-0">Add Subattribute</h4>
                                <div class="text-muted small mt-0 mb-4 d-block breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#"><i class="feather icon-home"></i></a>
                                        </li>
                                        <li class="breadcrumb-item"><a href="#">Subattribute</a></li>
                                        <li class="breadcrumb-item active">Add Subattribute</li>
                                    </ol>
                                </div>
                                <div>
                                    <div class="card mb-4"
                                        style="display:inline-block;margin:2%;width:25%;vertical-align:top;">
                                        <h3 class="card-header">Add New Subattribute</h3>
                                        <div class="card-body">
                                            <form method="post" enctype="multipart/form-data" id="sform">
                                                <div class="form-group">
                                                    <label class="form-label" style="width: 100%;">Select
                                                        Subattribute</label>
                                                    <select name="subattribute" id="subattribute"
                                                        style="width: 100%;padding: 5px;">
                                                        <option value="Select Value" selected disabled>Select Value
                                                        </option>
                                                        <% data.forEach(function(data) { %>
                                                            <option value="<%= data.aname %>">
                                                                <%= data.aname %>
                                                            </option>
                                                            <% })%>

                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label class="form-label w-100">Subattribute Name</label>
                                                    <input type="text" class="form-control" id="saname" name="saname"
                                                        autofocus="">
                                                </div>

                                                <button type="submit" class="btn btn-primary">ADD</button>
                                            </form>
                                        </div>
                                    </div>
                                    <div class="card mb-4" style="display:inline-block;margin:26px;width:65%">
                                        <div class="card">
                                            <div class="card-body">
                                                <!-- <div class="card col-md-2" id="bootstrap-table-toolbar"> -->
                                                    <button id="bootstrap-table-remove" onclick="delete_subattribute()"
                                                        class="btn btn-danger" style="margin-bottom: 30px;">
                                                        Delete</button>
                                                <!-- </div> -->
                                                <!-- DataTable within card -->

                                                <div class="card">

                                                    <div class="card-datatable table-responsive">

                                                        <table id="example" class="table table-striped"
                                                            style="width:100%">
                                                            <thead>
                                                                <tr>
                                                                    <th></th>
                                                                    <th></th>
                                                                    <th>Sr. No.</th>
                                                                    <th>Attribute Name</th>
                                                                    <th>Subattribute Name</th>

                                                                </tr>
                                                            </thead>

                                                           
                                                                    <tbody>
                                                                        <%var count=1%>
                                                                        
                                                                        <% datas.forEach(function(datas) { %>
                                                                        <tr>
                                                                            <td><input type="checkbox" name="check"
                                                                                    value="<%= datas.subattr_id  %>">
                                                                            </td>
                                                                            <td><a
                                                                                    href="/admin/subattribute_edit?id=<%= datas.subattr_id  %>"><button
                                                                                        type="button"
                                                                                        class="btn btn-info icon-btn borderless"
                                                                                        style="width: 50%;"><span
                                                                                            class="ion ion-md-create"></span></button></a>
                                                                            </td>

                                                                            <td>
                                                                                <%= count%>
                                                                                    <% count++ %>
                                                                            </td>
                                                                            <td>
                                                                            <%= datas.attr_id%>
                                                                                </td>
                                                                            <td>
                                                                            <%= datas.saname%>
                                                                                </td>

                                                                        </tr>
                                                                        <% }) %>
                                                                        
                                                                    </tbody>
                                                                   




                                                        </table>
                                                    </div>
                                                </div>

                                            </div>
                                            <!-- [ content ] End -->
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
